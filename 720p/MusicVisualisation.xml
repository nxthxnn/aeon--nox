<window id="2006">
	<allowoverlay>no</allowoverlay>
  <controls>
    <control type="group">
      <visible>IsEmpty(MusicPlayer.Property(Fanart_Image))</visible>
      <animation effect="zoom" start="110" end="130" center="auto" time="15000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
      <animation effect="slide" start="-10,-10" end="10,10" time="7000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
      <include>CommonMusicBackground</include>
    </control>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>$INFO[MusicPlayer.Property(Fanart_Image)]</texture>
			<include>Animation_VisibleChange400</include>
			<include>Animation_OpenCloseZoom</include>
      <animation effect="zoom" start="110" end="130" center="auto" time="15000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
      <animation effect="slide" start="-10,-10" end="10,10" time="7000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
      <visible>!IsEmpty(MusicPlayer.Property(Fanart_Image))</visible>
    </control>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>$INFO[Player.FolderPath]fanart.jpg</texture>
			<include>Animation_VisibleChange400</include>
			<include>Animation_OpenCloseZoom</include>
			<visible>Skin.HasSetting(albumlevelfanart)</visible>
			<animation effect="zoom" start="110" end="130" center="auto" time="15000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
			<animation effect="slide" start="-10,-10" end="10,10" time="7000" tween="cubic" easing="inout" pulse="true" condition="Skin.HasSetting(animation_bg)">Conditional</animation>
		</control>
    <control type="visualisation" id="2">
      <description>visualisation</description>
      <posx>0</posx>
      <posy>0</posy>
      <width>1280</width>
      <height>720</height>
    </control>
    <include condition="Skin.HasSetting(cdartmusicviz)">Original_Aeon_OSD</include>
    <include condition="!Skin.HasSetting(cdartmusicviz)">Aeon65_OSD</include>

	</controls>
</window>
