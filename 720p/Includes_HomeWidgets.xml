            <scrollspeed>50</scrollspeed>
            <pauseatend>5000</pauseatend>
            <visible>Control.HasFocus(8003)</visible>
          </control>
          <control type="image">
            <posx>77</posx>
            <posy>406</posy>
            <width>133</width>
